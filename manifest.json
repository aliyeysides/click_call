{
  "manifest_version": 2,

  "name": "clickcall",
  "description": "blah blah",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "make calls on one click",
    "default_popup": "popup.html"
  },
    "content_scripts": [ {
    "js": ["content_script.js" ],

    "matches": ["http://*/*", "https://*/*"]
  }],
  "background": {
    "scripts": ["popup.js"]
  },
  "permissions": [
  "tabs",
  "http://*/*"
  ]

}
